{
  "name": "administration-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@rmwc/button": "^6.1.4",
    "@rmwc/textfield": "^6.1.4",
    "@rmwc/theme": "^6.1.4",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.4",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "apollo-client": "^2.6.10",
    "dotenv": "^8.2.0",
    "formik": "^2.2.5",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "typescript": "^3.4.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start -r .env",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "ts-node --compiler-options \"{\\\"isolatedModules\\\":false}\" pre-generate.ts && graphql-codegen --config codegen.js",
    "lint": "eslint --cache --fix src --ext js,ts,tsx",
    "format": "prettier --write src --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@types/react-router-dom": "^5.1.6",
    "apollo": "^2.31.1",
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "stylelint": "^13.8.0",
    "stylelint-prettier": "^1.1.2",
    "ts-node": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  }
}
